{
  "name": "@hublo/panda-presets",
  "main": "src/index.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts"
  },
  "dependencies": {
    "@ark-ui/anatomy": "^0.1.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.15.5",
    "tsup": "7.2.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}
